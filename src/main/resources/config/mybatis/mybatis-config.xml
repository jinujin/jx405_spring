<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias type="com.bit.spring.model.UserDTO" alias="userDTO"/>
        <typeAlias type="com.bit.spring.model.BoardDTO" alias="boardDTO"/>
        <typeAlias type="com.bit.spring.model.GoodsDTO" alias="goodsDTO"/>
        <typeAlias type="com.bit.spring.model.BrandDTO" alias="brandDTO"/>
        <typeAlias type="com.bit.spring.model.BuyDTO" alias="buyDTO"/>
    </typeAliases>
    <mappers>
        <mapper resource="config/mybatis/mapper/UserMapper.xml"/>
        <mapper resource="config/mybatis/mapper/BoardMapper.xml"/>
        <mapper resource="config/mybatis/mapper/GoodsMapper.xml"/>
        <mapper resource="config/mybatis/mapper/BrandMapper.xml"/>
        <mapper resource="config/mybatis/mapper/BuyMapper.xml"/>
    </mappers>
</configuration>
